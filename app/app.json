{
  "expo": {
    "name": "Agori",
    "slug": "Agori",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "scheme": "agori",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-adaptive.png",
        "backgroundColor": "#8AD5A1"
      }
    }
  }
}
